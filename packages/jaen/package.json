    "react-redux": "^8.1.1",
    "redux": "^4.2.1",
    "uuid": "^9.0.0",
    "vanilla-cookieconsent": "^2.9.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
